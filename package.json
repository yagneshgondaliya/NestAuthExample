    "express": "^4.18.2",
    "fastify-formbody": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mysql": "^2.15.0",
    "mysql2": "^3.2.4",
    "owasp-password-strength-test": "^1.3.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^7.2.0",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
